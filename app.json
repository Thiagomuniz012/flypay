{
  "expo": {
    "name": "Fizpay",
    "slug": "fizpay",
    "version": "1.0.0",
    "description": "Fizpay",
    "scheme": "fizpay",
    "icon": "./assets/logo-fizpay-icon.png",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-fizpay-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fizpay.app"
    },
    "android": {
      "package": "com.fizpay.app",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-fizpay-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
